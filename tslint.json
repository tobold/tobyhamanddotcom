{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "interface-name": [false],
    "jsx-no-multiline-js": [false],
    "max-classes-per-file": [false, 1],
    "arrow-parens": [false],
    "prettier": [true]
  },
  "rulesDirectory": []
}
